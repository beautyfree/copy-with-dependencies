{
  "name": "copy-with-dependencies",
  "displayName": "Copy with Dependencies",
  "description": "Copy code fragments with all their dependencies",
  "version": "0.0.1",
  "publisher": "devall",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/beautyfree/copy-with-dependencies.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyWithDependencies.copy",
        "title": "Copy with Dependencies"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "copyWithDependencies.copy",
          "when": "editorHasSelection || editorTextFocus",
          "group": "copy"
        }
      ],
      "explorer/context": [
        {
          "command": "copyWithDependencies.copy",
          "when": "explorerResourceIsFile",
          "group": "copy"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish",
    "publish:patch": "vsce publish patch",
    "publish:minor": "vsce publish minor",
    "publish:major": "vsce publish major"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/vsce": "^3.7.1"
  },
  "dependencies": {
    "typescript": "^5.4.5"
  }
}